{
  "extends": [
    "warp/node",
    "warp/es6"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "semi": "off",
    "semi-style": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "line-comment-position": "off",
    "func-style": "off",
    "no-use-before-define": "off",
    "no-console": "off",
    "max-len": "off",
    "no-debugger": "warn",
    "complexity": "warn"
  }
}
